<div>
    <h1>Real Time Products</h1>

    <h2>PRODUCTOS</h2>
    <ul id="product-list">
        {{#each products}}
        <li id={{this._id}}>
            <h3>{{this.title}}</h3>
            <p>Precio: {{this.price}}</p>
            <p>Descripción: {{this.description}}</p>
            <p>Categoría: {{this.category}}</p>
        </li>
        {{/each}}
    </ul>

</div>


<script src="/socket.io/socket.io.js"></script>

<script src="/static/js/realtimeproducts.js"></script>